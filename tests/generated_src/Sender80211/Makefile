# generated by compiler
ROOT_PATH=$(COREVA_STREAMIT)
ifeq ($(COREVA_MPSOC_SIMFLAGS),)
	RUNTIME_DIR:=sim
else
	RUNTIME_DIR:=sim_$(shell echo '$(COREVA_MPSOC_SIMFLAGS)' | sed 's/[^a-zA-Z0-9]//g')
endif
%: 
	@mkdir -p $(RUNTIME_DIR) #remove this when there are dir guards in makebase_streamit
	@RUNTIME_DIR="${RUNTIME_DIR}" ROOT_PATH="${ROOT_PATH}" $(MAKE) --no-print-directory -C ${RUNTIME_DIR} -f $(COREVA_SOFTWARE)/makebase_streamit.mk $@
